RewriteEngine On

RewriteCond %{THE_REQUEST} !^GET\ /.*?static/(css|js|img)
RewriteCond %{THE_REQUEST} ^GET\ /(.*\.(png|jpg|jpeg|gif))
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ /library/$1 [L,QSA]

RewriteCond %{THE_REQUEST} !^GET\ /.*?static/(css|js|img)
RewriteCond %{THE_REQUEST} !^GET\ /.*\.(png|jpg|jpeg|gif)
